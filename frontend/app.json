{
  "expo": {
    "name": "Fridge to Food",
    "slug": "fridge-to-food",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EAF7EE"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.libowen.fridgetofood"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#EAF7EE"
      },
      "package": "com.libowen.fridgetofood"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      "expo-notifications",
      "expo-asset"
    ]
  }
}
